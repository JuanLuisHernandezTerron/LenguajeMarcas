<xs:schema
xmlns:xs="http://www.w3.org/2001/XMLSchema" 
elementFormDefault="qualified">
    <xs:element name="iceCreamShop">
            <xs:complexType>
                <xs:Sequence>
                 <xs:element nombre="iceCream" maxOccurs="Ubounded"/>
         <xs:ComplexType>
                <xs:Sequence>
                    <xs:element nombre="chocolate" Type="xs:integer"/>
                    <xs:element nombre="strawberry" Type="xs:integer"/>
                </xs:Sequence>
              <xs:attribute nombre="manufacture" Type="xs:date"/>
            </xs:ComplexType>
        </xs:Sequence>
      </xs:ComplexType>
   </xs:element>
</xs:schema>


        
